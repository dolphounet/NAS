
enable
write erase

conf t
mpls ip
mpls label protocol ldp
interface GigabitEthernet1/0
ip address 192.168.1.2 255.255.255.252
ip ospf 10 area 0
ip address 192.168.1.2 255.255.255.252
no shutdown
exit
interface GigabitEthernet2/0
ip address 192.168.1.5 255.255.255.252
ip ospf 10 area 0
ip address 192.168.1.5 255.255.255.252
no shutdown
exit
interface Loopback1
ip address 10.0.10.2 255.255.255.255
ip ospf 10 area 0
ip address 10.0.10.2 255.255.255.255
no shutdown
exit
router ospf 10
router-id 2.2.2.2
exit
end
