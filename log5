enable
write erase

conf t
interface GigabitEthernet1/0
ip address 100.100.0.2255.255.255.252
ip ospf 10 area 0
bandwidth 1000000
no shutdown
exit
interface Loopback1
ip address 10.0.20.1255.255.255.255
ip ospf 10 area 0
no shutdown
exit
vrf definition Client_0
rd 1:1
route-target export 100:2
route-target import 100:2
address-family ipv4
exit-address-family
exit
router bgp 2
bgp router-id 5.5.5.5
address-family vpnv4
exit-address-family
