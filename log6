
enable
write erase

conf t
interface GigabitEthernet1/0
ip address 100.100.46.2 255.255.255.0
vrf forwarding Client_0
no shutdown
exit
interface Loopback1
ip address 10.0.30.1 255.255.255.255
no shutdown
exit
router bgp 3
bgp router-id 6.6.6.6
neighbor 100.100.46.1 remote-as 1
address-family ipv4
redistribute connected
neighbor 100.100.46.1 activate
exit-address-family
exit
end
