enable
write erase

conf t
interface GigabitEthernet1/0
ip address 100.100.0.10255.255.255.252
ip ospf 10 area 0
bandwidth 1000000
no shutdown
exit
interface Loopback1
ip address 10.0.30.1255.255.255.255
ip ospf 10 area 0
no shutdown
exit
vrf definition Client_0
rd 1:4
route-target export 100:2
route-target import 100:2
address-family ipv4
exit-address-family
exit
router bgp 3
bgp router-id 6.6.6.6
address-family vpnv4
exit-address-family
